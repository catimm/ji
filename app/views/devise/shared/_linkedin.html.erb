
<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to omniauth_authorize_path(resource_name, provider), :class => "btn btn-block btn-social btn-linkedin" do %>
  		<i class="fa fa-linkedin"></i>Log in with <%= provider.to_s.titleize %>
  	<% end -%>
  <% end -%>
<% end -%>